{
  "name": "@pika/web",
  "version": "0.6.1",
  "description": "Install npm packages that run natively in the browser.",
  "author": "Fred K. Schott <fkschott@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pikapkg/web.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "pika build",
    "publish": "pika publish",
    "version": "npm run build"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-copy-assets"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-simple-bin",
        {
          "bin": "pika-web"
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@pika/cli": "latest",
    "chalk": "^2.4.2",
    "glob": "^7.1.4",
    "is-builtin-module": "^3.0.0",
    "mkdirp": "^0.5.1",
    "ora": "^4.0.2",
    "resolve-from": "^5.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.23.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "yargs-parser": "^15.0.0"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.6.1",
    "@pika/plugin-copy-assets": "^0.6.1",
    "@pika/plugin-simple-bin": "^0.6.1",
    "@pika/plugin-ts-standard-pkg": "^0.6.1",
    "@types/mkdirp": "^0.5.2",
    "prettier": "^1.18.2",
    "typescript": "^3.6.4"
  }
}
