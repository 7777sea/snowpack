{
  "name": "@pika/web",
  "version": "0.6.1",
  "description": "Install npm packages that run natively in the browser.",
  "author": "Fred K. Schott <fkschott@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pikapkg/web.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "pika build",
    "lint": "prettier --check 'src/**/*.ts'",
    "publish": "pika publish",
    "version": "npm run build",
    "test": "jest __tests__/runner.js"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-copy-assets"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-simple-bin",
        {
          "bin": "pika-web"
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/parser": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/traverse": "^7.6.3",
    "@babel/types": "^7.6.3",
    "@pika/cli": "latest",
    "chalk": "^2.4.2",
    "dir-compare": "^1.8.0",
    "glob": "^7.1.4",
    "is-builtin-module": "^3.0.0",
    "mkdirp": "^0.5.1",
    "ora": "^3.1.0",
    "resolve-from": "^5.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.2.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^5.1.1",
    "yargs-parser": "^13.0.0"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.6.1",
    "@pika/plugin-copy-assets": "^0.6.1",
    "@pika/plugin-simple-bin": "^0.6.1",
    "@pika/plugin-ts-standard-pkg": "^0.6.1",
    "@types/babel__traverse": "^7.0.7",
    "@types/mkdirp": "^0.5.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "typescript": "^3.6.4"
  }
}
